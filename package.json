{
  "name": "fly",
  "version": "2.0.6-beta",
  "description": "Modern Build System",
  "license": "MIT",
  "repository": "flyjs/fly",
  "homepage": "https://github.com/flyjs/fly",
  "author": {
    "name": "Jorge Bucaran",
    "email": "j@bucaran.me",
    "url": "https://github.com/bucaran"
  },
  "contributors": [
    {
      "name": "Luke Edwards",
      "email": "luke@lukeed.com",
      "url": "https://lukeed.com"
    }
  ],
  "main": "lib/index.js",
  "bin": "cli.js",
  "files": [
    "lib",
    "cli.js"
  ],
  "dependencies": {
    "bluebird": "^3.4.7",
    "chalk": "^1.1.3",
    "find-up": "^2.1.0",
    "glob": "^7.1.1",
    "minimist": "^1.2.0",
    "mkdirp": "^0.5.1",
    "prettyjson": "^1.1.2"
  },
  "devDependencies": {
    "rimraf": "^2.5.4",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.3",
    "xo": "^0.12.0"
  },
  "scripts": {
    "test": "xo && tape test/*.js | tap-spec"
  },
  "keywords": [
    "cli",
    "task",
    "build",
    "async",
    "await",
    "minify",
    "uglify",
    "promise",
    "pipeline",
    "generator",
    "coroutine",
    "automation",
    "task runner",
    "build system"
  ],
  "engines": {
    "node": ">= 4.6.0"
  },
  "xo": {
    "esnext": true,
    "ignore": [
      "examples/**/*.js"
    ],
    "rules": {
      "no-use-extend-native/no-use-extend-native": 0,
      "no-unused-expressions": 1,
      "prefer-spread": 1,
      "guard-for-in": 1,
      "no-loop-func": 0
    }
  }
}
